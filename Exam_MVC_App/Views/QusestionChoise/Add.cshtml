@model Question_Choice


<h1>Create Cource</h1>

<form method="POST" action="/QusestionChoise/AddNew">
    <div class="form-horizontal">
        <h4>Track</h4>
        <hr />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="Choice" class="control-label"></label>
            <input asp-for="Choice" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Choice_Type" class="control-label"></label>
            <input asp-for="Choice_Type" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Question_Id" class="control-label"></label>
            <input asp-for="Question_Id" class="form-control" />
        </div>
		<div class="form-group">
			<input type="submit" value="Create" class="btn btn-primary" />
		</div>
	</div>
   
</form>